{
  "name": "agif",
  "version": "0.0.0",
  "private": true,
  "description": "split and reverse animated gif images",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaharu/agif.git"
  },
  "author": "aaharu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaharu/agif/issues"
  },
  "scripts": {
    "dev": "npm run clean && parcel serve -d public/dist views/*.html",
    "build": "npm run clean && parcel build -d public/dist --public-url '/dist/' views/*.html",
    "clean": "rimraf public/dist",
    "lint": "eslint --fix ./src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0-alpha.7",
    "gifken": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "acorn": "^6.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "parcel-bundler": "^1.12.3",
    "prettier": "1.17.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/react",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-constant-condition": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/adjacent-overload-signatures": "error"
    }
  }
}
