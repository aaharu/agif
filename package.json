{
  "name": "agif",
  "version": "0.0.0",
  "private": true,
  "description": "split and reverse animated gif images",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaharu/agif.git"
  },
  "author": "aaharu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaharu/agif/issues"
  },
  "scripts": {
    "dev": "npm run clean && parcel serve -d public/dist views/*.html",
    "build": "npm run clean && parcel build -d public/dist --public-url '/dist/' views/*.html",
    "clean": "rimraf public/dist",
    "lint": "eslint --fix ./src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "gifken": "^2.1.1",
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3 || ^3.0.0",
    "typescript": "~3.8.3"
  }
}
